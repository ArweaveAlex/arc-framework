{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es6",
		"lib": ["dom", "dom.iterable", "esnext"],
		"moduleResolution": "node",
		"outDir": "dist",
		"strict": true,
		"declaration": true,
		"esModuleInterop": true,
		"noUnusedLocals": false,
		"noUnusedParameters": false,
		"baseUrl": "."
	},
	"include": ["src/**/*.ts"],
	"exclude": ["node_modules", "**/*.spec.ts"]
}
